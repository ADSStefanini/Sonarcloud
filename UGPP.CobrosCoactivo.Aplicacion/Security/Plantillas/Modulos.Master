<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="Modulos.master.vb" Inherits="coactivosyp.Modulos" %>
<%@ Register TagPrefix="HeadControl" TagName="HeadScripts" Src="~/Security/Controles/commons-head.ascx" %>
